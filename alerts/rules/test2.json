{
  "rule_name": "test2",
  "rule_author": "siembol",
  "rule_version": 1,
  "rule_description": "AWS failed authentication12",
  "source_type": "aws_cloudtrail",
  "matchers": [
    {
      "is_enabled": true,
      "matcher_type": "REGEX_MATCH",
      "is_negated": false,
      "field": "eventName",
      "data": "(?i)ConsoleLogin$"
    },
    {
      "is_enabled": true,
      "matcher_type": "IS_IN_SET",
      "is_negated": false,
      "field": "errorMessage",
      "case_insensitive": true,
      "data": "Failed authentication"
    }
  ],
  "tags": [
    {
      "tag_name": "correlation_key",
      "tag_value": "${userIdentity_principalId}"
    }
  ]
}